{
  "name": "genieshopper",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.1"},
  "description": "Electricity Program Shopping Assistant",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "client": "cd client && npm run start",
    "start": "node server.js",
    "server": "nodemon server.js",
    "install": "cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
   "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lokibeat/GenieShopper.git"
  },
  "author": "Everard Santamarina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lokibeat/GenieShopper/issues"
  },
  "homepage": "https://github.com/lokibeat/GenieShopper#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "chart.js": "^2.7.2",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
